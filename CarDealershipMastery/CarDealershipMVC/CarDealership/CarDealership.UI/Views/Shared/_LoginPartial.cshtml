@using Microsoft.AspNet.Identity
@if (Request.IsAuthenticated)
{
	<ul class="nav navbar-nav navbar-right">
		<li>
			<a href="#">Hello @User.Identity.GetUserName()!</a>
		</li>
	</ul>
}
else
{
	<ul class="nav navbar-nav navbar-right">
		<li>@Html.ActionLink("Login", "Login", "Account")</li>
	</ul>
}
